import pandas as pd
import dataiku

# Créer un DataFrame avec les données à insérer
data_to_insert = pd.DataFrame({
    "Nom": ["Nomi", "Nom2", "Nom3"],
    "Prenom": ["Prenomi", "Prenon2", "Prenom3"],
    "Age": [25, 30, 22]
})

# Obtenir le dataset (table) existant dans votre projet
project_key = "VOTRE_CLE_DE_PROJET"
dataset_name = "NOM_DU_DATASET"  # Le nom de la table dans Dataiku

dataset = dataiku.Dataset(project_key, dataset_name)

# Insérer les données dans la table
dataset.write_with_schema(data_to_insert)
